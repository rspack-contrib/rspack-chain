{
  "name": "rspack-chain",
  "version": "1.4.1",
  "keywords": [
    "rspack",
    "config",
    "chain",
    "api"
  ],
  "repository": "rspack-contrib/rspack-chain",
  "license": "MIT",
  "author": "rspack-contrib",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "rslib build",
    "bump": "npx bumpp",
    "dev": "rslib build -w",
    "fix": "yarn lint:fix && yarn style:fix",
    "prepare": "pnpm build",
    "style": "prettier --check .",
    "style:fix": "prettier --write .",
    "test": "rstest run",
    "test:types": "tsc -p ./types/test/tsconfig.json"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "javascript-stringify": "^2.1.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.11.0",
    "@rspack/core": "^1.5.0-beta.0",
    "@rstest/core": "^0.0.10",
    "prettier": "^3.6.2",
    "typescript": "^5.9.2",
    "webpack": "^5.101.0"
  },
  "packageManager": "pnpm@10.14.0"
}
