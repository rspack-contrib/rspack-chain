{
  "name": "rspack-chain",
  "version": "1.4.4",
  "keywords": [
    "rspack",
    "config",
    "chain",
    "api"
  ],
  "repository": "rstackjs/rspack-chain",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "typings": "types/index.d.ts",
  "files": [
    "dist",
    "types/*.d.ts"
  ],
  "scripts": {
    "build": "rslib build",
    "bump": "npx bumpp",
    "dev": "rslib build -w",
    "fix": "yarn lint:fix && yarn style:fix",
    "prepare": "pnpm build",
    "style": "prettier --check .",
    "style:fix": "prettier --write .",
    "test": "rstest run",
    "test:types": "tsc -p ./types/test/tsconfig.json"
  },
  "dependencies": {
    "deepmerge": "^4.3.1",
    "javascript-stringify": "^2.1.0"
  },
  "devDependencies": {
    "@rslib/core": "^0.18.2",
    "@rspack/core": "2.0.0-canary-20260116",
    "@rstest/core": "^0.6.8",
    "prettier": "^3.7.3",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0"
  },
  "peerDependencies": {
    "@rspack/core": "^1.0.0 || ^2.0.0 || ^2.0.0-0"
  },
  "peerDependenciesMeta": {
    "@rspack/core": {
      "optional": true
    }
  },
  "packageManager": "pnpm@10.24.0"
}
